    "steps": [
        {
            "tag": "ol",
            "steps": [
                {
                    "tag": "li",
                    "text": "Head to the \u25c0Yasna Monument\u25b6\u25c1https://genshin-impact.fandom.com/wiki/Yasna_Monument\u25b7 and look for \u25c0Aranakula\u25b6\u25c1https://genshin-impact.fandom.com/wiki/Aranakula\u25b7"
                },
                {
                    "tag": "li",
                    "text": "Look for Aranakula nearby"
                },
                {
                    "tag": "li",
                    "text": "Talk to Aranakula"
                },
                {
                    "tag": "li",
                    "text": "Move forward without being noticed"
                },
                {
                    "tag": "ul",
                    "steps": [
                        {
                            "tag": "li",
                            "text": "If Aranakula runs away, return to the last location where Aranakula speaks with the player (\"safe area\") to continue."
                        },
                        {
                            "tag": "li",
                            "text": "Aranakula cannot follow across water and will return to the last safe area once the player exits the water."
                        },
                        {
                            "tag": "li",
                            "text": "There is no point in defeating the Fungi as they do not drop materials and new Fungi will spawn."
                        }
                    ]
                },
                {
                    "tag": "li",
                    "text": "Go to the place that Aranakula mentioned"
                },
                {
                    "tag": "li",
                    "text": "Clear the spores that cover the stones"
                },
                {
                    "tag": "li",
                    "text": "Find the \"branch\" together with Aranakula"
                },
                {
                    "tag": "ul",
                    "steps": [
                        {
                            "tag": "li",
                            "text": "The \"branch\" is located inside a cave. The entrance is at southwest of the hill, under a \u25c0Four-Leaf Sigil\u25b6\u25c1https://genshin-impact.fandom.com/wiki/Four-Leaf_Sigil\u25b7."
                        }
                    ]
                },
                {
                    "tag": "li",
                    "text": "Make the three \"leaves\" turn towards the \"branch\""
                },
                {
                    "tag": "ul",
                    "steps": [
                        {
                            "tag": "li",
                            "text": "Follow the \u25c0Seelie\u25b6\u25c1https://genshin-impact.fandom.com/wiki/Seelie\u25b7 from the cave entrance to the three \"leaves.\""
                        }
                    ]
                },
                {
                    "tag": "li",
                    "text": "Talk to Aranakula"
                }
            ]
        }
    ],
    "dialogue": true
